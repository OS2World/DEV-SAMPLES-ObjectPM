MENU MAINMENU 
BEGIN
	SUBMENU "~File", M_FILE
	BEGIN
		MENUITEM "~New...",  MI_NEWFILE
		MENUITEM "~Open...", MI_OPENFILE
		MENUITEM SEPARATOR
		MENUITEM "~Save",	 MI_SAVEFILE
		MENUITEM "Save ~as...", MI_SAVEAS
		MENUITEM SEPARATOR
		MENUITEM "Name...",	 MI_SETPHONEBOOKNAME
		MENUITEM "Print...", MI_PRINTPHONEBOOK
		MENUITEM "Print setup...", MI_PRINTSETUP
	END

	SUBMENU "~Item", M_ITEM
	BEGIN
		MENUITEM "~Add...", 	 MI_ADDENTRY
		MENUITEM "~Find...",	 MI_FINDENTRY	
		MENUITEM SEPARATOR
		MENUITEM "~Copy...", 	 MI_COPYENTRY
		MENUITEM "~Delete", 	 MI_DELETEENTRY
	END

	SUBMENU "~Options", M_OPTIONS
	BEGIN
		SUBMENU "~View", M_OPTIONS + 1
		BEGIN
			MENUITEM "~Details", MI_SETDETAILSVIEW
			MENUITEM "~Icon", 	 MI_SETICONVIEW
		END
		MENUITEM "~Tool bar", 	 MI_TOGGLETOOLBAR
		MENUITEM "~Status bar",  MI_TOGGLESTATUSBAR
		MENUITEM "S~ave",		 1000
	END

	SUBMENU "~Help", M_HELP
	BEGIN
		MENUITEM "Help ~index...", 401
		MENUITEM "~General help...", 402
		MENUITEM "~Using help...", 403
		MENUITEM "~Keys help...", 404
		MENUITEM SEPARATOR
		MENUITEM "~Product information...", 405
	END
END

MENU MP_PHONELIST 
BEGIN
	SUBMENU "~View", 200
	BEGIN
		MENUITEM "~Details", MI_SETDETAILSVIEW
		MENUITEM "~Icon", 	 MI_SETICONVIEW
	END

	SUBMENU "~Print", 300
	BEGIN
		MENUITEM "~Phone book",		  MI_PRINTPHONEBOOK
		MENUITEM "~Selected item(s)", MI_PRINTSEL
	END

	MENUITEM "~Name...", 	MI_SETPHONEBOOKNAME
	MENUITEM "~Arrange", 	MI_ARRANGEICONS
	MENUITEM SEPARATOR
	MENUITEM "~Add entry...", 	MI_ADDENTRY
	MENUITEM "~Find entry...", 	MI_FINDENTRY
END

