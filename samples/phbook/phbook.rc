STRINGTABLE LOADONCALL
BEGIN
	MI_NEWFILE				"Start a new phone book"			
	MI_OPENFILE				"Open an existing phone book"
	MI_SAVEFILE				"Save the current phone book"
	MI_SAVEAS				"Save the current phbone book as..."
	MI_PRINTPHONEBOOK		"Print selected or all phone book items"
	MI_SETDETAILSVIEW		"Display entries in details view"
	MI_SETICONVIEW			"Display entries in icon view"
	MI_ADDENTRY				"Add a new phone book entry"
	MI_DELETEENTRY			"Delete selected phone book entries"
	MI_COPYENTRY			"Copy this phone book entry"
	MI_FINDENTRY			"Find an entry"
	MI_TOGGLESTATUSBAR		"Turn this status bar on or off"
	MI_TOGGLETOOLBAR		"Turn the tool bar on or off"
	MI_PRINTITEM			"Print this phone book entry"
	MI_SETPHONEBOOKNAME		"Change the name of the phone book"
	MI_ARRANGEICONS			"Arrange the icons in icon view"
	MI_PRINTSETUP			"Set the printer queue & job properties"
	MI_SAVESETUP			"Save the phone book setup parameters"
	MI_PRINTSEL				"Print the selected item(s)"
END

